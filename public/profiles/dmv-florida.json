{
  "id": "dmv-florida",
  "name": "Florida DMV",
  "description": "Florida Department of Highway Safety and Motor Vehicles",
  "icon": "ðŸš—",
  "numbers": ["+16037184055"],
  "businessHours": {
    "timezone": "America/New_York",
    "monday": { "enabled": true, "start": "08:00", "end": "17:00" },
    "tuesday": { "enabled": true, "start": "08:00", "end": "17:00" },
    "wednesday": { "enabled": true, "start": "08:00", "end": "17:00" },
    "thursday": { "enabled": true, "start": "08:00", "end": "17:00" },
    "friday": { "enabled": true, "start": "08:00", "end": "17:00" },
    "saturday": { "enabled": false, "start": "09:00", "end": "13:00" },
    "sunday": { "enabled": false, "start": "09:00", "end": "13:00" }
  },
  "patterns": [
    {
      "id": "agents-busy",
      "name": "Agents Busy",
      "detect": [
        "all agents are",
        "agents are currently busy",
        "call back later",
        "try your call later",
        "try your call again",
        "please call back",
        "try again later"
      ],
      "action": "notify",
      "value": 60,
      "autoHangup": false,
      "description": "Notify when agents are busy but stay connected"
    },
    {
      "id": "dmv-greeting",
      "name": "DMV Initial Greeting",
      "detect": [
        "customer service center",
        "florida department of highway",
        "department of highway safety",
        "your calls important to us",
        "please listen carefully"
      ],
      "action": "wait",
      "value": 5,
      "description": "Wait for menu options to begin"
    },
    {
      "id": "menu-options",
      "name": "Menu Options",
      "detect": [
        "dial 1",
        "press 1",
        "dial one",
        "press one",
        "for english",
        "to continue in english",
        "make your selection",
        "menu option"
      ],
      "action": "say",
      "value": "Agent",
      "delay": 1,
      "description": "Say 'Agent' when menu options are presented"
    },
    {
      "id": "agent-acknowledgment",
      "name": "Agent Request Acknowledged",
      "detect": [
        "i understand you want to speak",
        "you want to speak with an agent",
        "speak to an agent",
        "speak with a representative",
        "i understand you want an agent"
      ],
      "action": "wait",
      "value": 3,
      "then": {
        "action": "listen",
        "for": "menu-options-repeat"
      },
      "description": "Wait for menu to repeat after acknowledgment"
    },
    {
      "id": "menu-options-repeat",
      "name": "Menu Options (Second Time)",
      "detect": [
        "dial 1",
        "press 1",
        "dial one",
        "press one",
        "for english",
        "to continue"
      ],
      "action": "say",
      "value": "Agent",
      "delay": 1,
      "description": "Say 'Agent' again when menu repeats"
    },
    {
      "id": "human-detected",
      "name": "Human Agent",
      "detect": [
        "how can i help",
        "speaking",
        "this is",
        "good morning",
        "good afternoon",
        "hello",
        "yes?"
      ],
      "action": "connect",
      "record": true,
      "alert": true,
      "description": "Connect browser audio and start recording when human detected"
    }
  ],
  "redialSettings": {
    "maxAttempts": 20,
    "delaySeconds": 60,
    "exponentialBackoff": false
  },
  "recordingSettings": {
    "enabled": true,
    "onlyHuman": true,
    "minDuration": 2
  }
}